<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<model>
    <version>v1</version>
    <datasources>
        <datasource name="h2" class="com.platum.restflow.resource.impl.jdbc.JdbcRepository">
            <properties>
                <property name="jdbcUrl" value="jdbc:h2:mem:test;INIT=create table product(id integer, name varchar(50));"/>
                <property name="driverClassName" value="org.h2.Driver" />
                <property name="username" value="sa"/>
                <property name="password" value=""/>         
            </properties>
        </datasource>
    </datasources>
    <filesystems>
        <filesytem name="localFs">
            <properties>
                <property name="path" value="./_files"/>
            </properties>
        </filesytem>
    </filesystems>        
</model>
